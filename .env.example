# KrpoProdaja API Configuration
# Copy this file to .env.local and update with your values

# API Base URL
NEXT_PUBLIC_API_URL=http://localhost:8080/api

# Optional: API timeout in milliseconds
NEXT_PUBLIC_API_TIMEOUT=10000

# Backend API URL for server-side validation (used in middleware)
# This should point to your krpoprodaja-api backend
BACKEND_API_URL=http://localhost:8080

# OAuth Configuration
# Google OAuth Client ID (get from https://console.cloud.google.com/apis/credentials)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook App ID (get from https://developers.facebook.com/apps/)
NEXT_PUBLIC_FACEBOOK_APP_ID=your-facebook-app-id
